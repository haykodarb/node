{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@now/static" },
      { "src": "*.png", "use": "@now/static" },
      { "src": "*.jpg", "use": "@now/static" },
      { "src": "*.ico", "use": "@now/static" },
      { "src": "*.js", "use": "@now/node" }
    ],
    "routes": [
      { "src": "/datos", "dest": "server.js" },
      { "src": "/insert", "dest": "server.js" },
      { "src": "/graficos", "dest": "server.js" },
      { "src": "/graficos/hoy", "dest": "server.js" },
      { "src": "/graficos/semana", "dest": "server.js" },
      { "src": "/graficos/mes", "dest": "server.js" },
      { "src": "/favicon.ico", "dest": "favicon.ico"}
      ]
  }
